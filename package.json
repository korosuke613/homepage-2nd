{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": "Futa Hirakoba (https://github.com/korosuke613)",
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "clean": "rimraf dist",
    "dev": "astro dev",
    "lint": "concurrently -P npm:lint:*",
    "lint-fix": "concurrently -P 'npm:lint:* -- {1}' -- --fix",
    "lint:astro": "eslint --ext .astro .",
    "lint:normal": "eslint --ext .js,.jsx,.ts,.tsx, .",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "jest --ci"
  },
  "dependencies": {
    "@astrojs/mdx": "0.19.7",
    "@astrojs/partytown": "1.2.3",
    "@astrojs/react": "2.2.2",
