{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": {
    "name": "Futa Hirakoba",
    "url": "https://github.com/korosuke613"
  },
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "db:update": "astro db push --remote && astro db execute db/updateGA4Data.ts --remote",
    "dev": "astro dev",
    "lint": "biome ci .",
    "lint:fix": "biome check --write ./src ./db ./.storybook ./vite.config.mts ./vitest.shims.d.ts",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "vitest run --project=unit --coverage",
    "test:storybook": "vitest run --project=storybook --coverage",
    "vrt:init": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/init.spec.ts",
    "vrt:regression": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/regression.spec.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --debug --disable-telemetry",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/db": "^0.15.0",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/partytown": "2.1.4",
    "@astrojs/react": "4.3.0",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@docsearch/css": "3.9.0",
    "@docsearch/react": "3.9.0",
    "@google-analytics/data": "4.12.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.16",
    "astro": "^5.10.1",
    "astro-meta-tags": "0.3.2",
    "astro-robots-txt": "1.0.0",
    "date-fns": "4.1.0",
    "emoji-regex": "10.4.0",
    "glob": "11.0.3",
    "natural": "8.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "5.4.0",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.1",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "simple-git": "3.27.0",
    "strip-ansi": "7.1.0",
    "tailwindcss": "3.4.17",
    "typescript": "^5.2.2",
    "unified": "11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@chromatic-com/storybook": "4.0.1",
    "@playwright/experimental-ct-react": "1.54.1",
    "@playwright/test": "1.46.1",
    "@proofdict/textlint-rule-proofdict": "3.1.2",
    "@storybook/addon-a11y": "9.0.14",
    "@storybook/addon-docs": "9.0.14",
    "@storybook/addon-links": "9.0.14",
    "@storybook/addon-vitest": "^9.0.14",
    "@storybook/react-vite": "9.0.14",
    "@types/react": "19.1.8",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "chromatic": "11.29.0",
    "concurrently": "9.2.0",
    "playwright": "1.46.1",
    "storybook": "9.0.14",
    "textlint": "14.8.4",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "10.0.2",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
    "vite-plugin-turbosnap": "1.0.3",
    "vitest": "3.2.4"
  }
}
