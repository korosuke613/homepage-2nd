{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": {
    "name": "Futa Hirakoba",
    "url": "https://github.com/korosuke613"
  },
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "db:update": "astro db push --remote && astro db execute db/updateGA4Data.ts --remote",
    "dev": "astro dev",
    "lint": "concurrently -P 'npm:lint:*(!fix)'",
    "lint:astro": "eslint --ext .astro ./src",
    "lint:js": "biome ci .",
    "lint:fix": "concurrently -P 'npm:lint:fix:*'",
    "lint:fix:astro": "concurrently -P 'npm:lint:astro -- {1}' -- --fix",
    "lint:fix:js": "biome check --apply ./src ./db",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "vitest run --coverage",
    "vrt:init": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/init.spec.ts",
    "vrt:regression": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/regression.spec.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --debug --disable-telemetry",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@astrojs/check": "0.5.10",
    "@astrojs/db": "0.11.2",
    "@astrojs/mdx": "2.3.1",
    "@astrojs/partytown": "2.1.0",
    "@astrojs/react": "3.3.4",
    "@astrojs/rss": "4.0.6",
    "@astrojs/sitemap": "3.1.4",
    "@astrojs/tailwind": "5.1.0",
    "@docsearch/css": "3.6.0",
    "@docsearch/react": "3.6.0",
    "@google-analytics/data": "4.5.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.13",
    "astro": "4.7.1",
    "astro-meta-tags": "^0.2.1",
    "astro-robots-txt": "1.0.0",
    "date-fns": "3.6.0",
    "emoji-regex": "10.3.0",
    "glob": "10.3.15",
    "natural": "7.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.0",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "simple-git": "3.24.0",
    "strip-ansi": "7.1.0",
    "tailwindcss": "3.4.3",
    "typescript": "^5.2.2",
    "unified": "11.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@chromatic-com/storybook": "1.4.0",
    "@playwright/experimental-ct-react": "1.44.0",
    "@playwright/test": "1.44.0",
    "@proofdict/textlint-rule-proofdict": "3.1.2",
    "@storybook/addon-a11y": "8.0.10",
    "@storybook/addon-essentials": "8.0.10",
    "@storybook/addon-interactions": "8.0.10",
    "@storybook/addon-links": "8.0.10",
