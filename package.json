{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": {
    "name": "Futa Hirakoba",
    "url": "https://github.com/korosuke613"
  },
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "dev": "astro dev",
    "lint": "concurrently -P 'npm:lint:*(!fix)'",
    "lint:astro": "eslint --ext .astro ./src",
    "lint:js": "biome ci .",
    "lint:fix": "concurrently -P 'npm:lint:fix:*'",
    "lint:fix:astro": "concurrently -P 'npm:lint:astro -- {1}' -- --fix",
    "lint:fix:js": "biome check --apply ./src",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "vitest run --coverage",
    "vrt:init": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/init.spec.ts",
    "vrt:regression": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/regression.spec.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --debug --disable-telemetry",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@astrojs/check": "0.5.6",
    "@astrojs/mdx": "2.1.1",
    "@astrojs/partytown": "2.0.4",
    "@astrojs/react": "3.0.10",
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "3.1.1",
    "@astrojs/tailwind": "5.1.0",
    "@docsearch/css": "3.5.2",
    "@docsearch/react": "3.5.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.10",
    "astro": "4.4.6",
    "astro-meta-tags": "^0.2.1",
    "astro-robots-txt": "1.0.0",
    "date-fns": "3.3.1",
    "eastasianwidth": "0.2.0",
    "emoji-regex": "10.3.0",
    "glob": "10.3.10",
    "natural": "6.10.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.0",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "simple-git": "3.22.0",
    "strip-ansi": "7.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "^5.2.2",
    "unified": "11.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@playwright/experimental-ct-react": "1.41.2",
    "@playwright/test": "1.41.2",
    "@proofdict/textlint-rule-proofdict": "3.1.2",
    "@storybook/addon-a11y": "7.6.17",
    "@storybook/addon-console": "3.0.0",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/addon-storysource": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.17",
    "@storybook/test": "7.6.17",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.2.61",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitest/coverage-v8": "1.3.1",
    "@vitest/ui": "1.3.1",
    "chromatic": "11.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-tailwindcss": "3.14.3",
    "eslint-plugin-unused-imports": "3.1.0",
    "playwright": "1.41.2",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "storybook": "7.6.17",
    "textlint": "14.0.3",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-spacing": "2.3.0",
    "textlint-rule-preset-ja-technical-writing": "10.0.1",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
    "vite-plugin-turbosnap": "1.0.3",
    "vitest": "1.3.1"
  }
}
