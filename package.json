{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": "Futa Hirakoba (https://github.com/korosuke613)",
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "clean": "rimraf dist",
    "dev": "astro dev",
    "lint": "concurrently -P 'npm:lint:*(!fix)'",
    "lint:astro": "eslint --ext .astro ./src",
    "lint:js": "biome ci .",
    "lint:fix": "concurrently -P 'npm:lint:fix:*'",
    "lint:fix:astro": "concurrently -P 'npm:lint:astro -- {1}' -- --fix",
    "lint:fix:js": "biome check --apply ./src",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "jest --ci"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/mdx": "1.1.0",
    "@astrojs/partytown": "2.0.0",
    "@astrojs/react": "3.0.2",
    "@astrojs/rss": "3.0.0",
    "@astrojs/sitemap": "3.0.0",
    "@astrojs/tailwind": "5.0.0",
    "@docsearch/css": "3.5.2",
    "@docsearch/react": "3.5.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.10",
    "astro": "3.2.0",
    "astro-robots-txt": "1.0.0",
    "date-fns": "2.30.0",
    "eastasianwidth": "0.2.0",
    "emoji-regex": "10.2.1",
    "glob": "10.3.10",
    "natural": "6.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.0",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.0.0",
    "simple-git": "3.20.0",
    "strip-ansi": "7.1.0",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2",
    "unified": "11.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.2.2",
    "@playwright/experimental-ct-react": "1.38.1",
    "@playwright/test": "1.38.1",
    "@proofdict/textlint-rule-proofdict": "3.1.2",
    "@swc/core": "1.3.90",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.23",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "bun-types": "1.0.3",
    "concurrently": "8.2.1",
    "eslint": "8.50.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "playwright": "1.38.1",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "rimraf": "5.0.5",
    "textlint": "13.3.3",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-spacing": "2.3.0",
    "textlint-rule-preset-ja-technical-writing": "8.0.0",
    "textlint-rule-spellcheck-tech-word": "5.0.0"
  }
}
