{
  "name": "korosuke613-homepage",
  "version": "2.0.0",
  "private": true,
  "author": {
    "name": "Futa Hirakoba",
    "url": "https://github.com/korosuke613"
  },
  "scripts": {
    "build": "astro check && tsc --noEmit && astro build",
    "build-types": "tsc --noEmit --pretty",
    "db:update": "astro db push --remote && astro db execute db/updateGA4Data.ts --remote",
    "dev": "astro dev",
    "lint": "biome ci .",
    "lint:fix": "biome check --apply ./src ./db",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "concurrently -P npm:test:*",
    "test:playwright-ct": "playwright test -c playwright-ct.config.ts",
    "test:playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "test:unit": "vitest run --coverage",
    "vrt:init": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/init.spec.ts",
    "vrt:regression": "playwright test -c playwright-vrt.config.ts ./src/tests/vrt/regression.spec.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --debug --disable-telemetry",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/db": "0.11.4",
    "@astrojs/mdx": "3.1.3",
    "@astrojs/partytown": "2.1.1",
    "@astrojs/react": "3.5.0",
    "@astrojs/rss": "4.0.7",
    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.0",
    "@docsearch/css": "3.6.1",
    "@docsearch/react": "3.6.1",
    "@google-analytics/data": "4.7.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.13",
    "astro": "4.10.1",
    "astro-meta-tags": "0.3.0",
    "astro-robots-txt": "1.0.0",
    "date-fns": "3.6.0",
    "emoji-regex": "10.3.0",
    "glob": "10.4.5",
    "natural": "7.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.0",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "simple-git": "3.25.0",
    "strip-ansi": "7.1.0",
    "tailwindcss": "3.4.6",
    "typescript": "^5.2.2",
    "unified": "11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@chromatic-com/storybook": "1.6.1",
    "@playwright/experimental-ct-react": "1.45.3",
    "@playwright/test": "1.45.3",
    "@proofdict/textlint-rule-proofdict": "3.1.2",
    "@storybook/addon-a11y": "8.2.6",
    "@storybook/addon-essentials": "8.2.6",
    "@storybook/addon-interactions": "8.2.6",
    "@storybook/addon-links": "8.2.6",
    "@storybook/addon-storysource": "8.2.6",
    "@storybook/blocks": "8.2.6",
    "@storybook/react": "8.2.6",
    "@storybook/react-vite": "8.2.6",
    "@storybook/test": "8.2.6",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.3.3",
    "@vitest/coverage-v8": "2.0.3",
    "@vitest/ui": "2.0.3",
    "chromatic": "11.5.6",
    "concurrently": "8.2.2",
    "playwright": "1.45.3",
    "storybook": "8.2.6",
    "textlint": "14.0.4",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "10.0.1",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
